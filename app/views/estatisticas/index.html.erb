<div class="row"> 
    <div class="x_panel" STYLE="font-family: Gilroy;MARGIN-LEFT:14PX;width: 98.5%;box-shadow: 0px 0px 3px #415467;">
        <div class="x_title" style="font-family: Gilroy ExtraBold;color: #1abb9c;">
            <h2><i class="fa fa-tasks" style="margin-right:10px"></i>Atividade da semana<small><%= Time.now.strftime("%d/%m/%Y")%></small></h2> 
            <div class="clearfix"></div>
        </div> 
            <div class="dashboard-widget-content" style="margin-top: -51px;font-size: 20px;margin-left: -30px;">

                <ul class="list-unstyled timeline widget">
                    <%= week_calendar events: @atividades do |date, atividades| %> 
                        <% atividades.each do |atividade| %>  
                            <% if atividade.status == "Aberto"%>  
                                <li style="display: block;margin-left: -14px;border-bottom: 0px solid;">  
                                    <div class="block" style="width: 100%;
    border-left: 0px solid #eeeeee;
    /* border-bottom: 2px solid #4a6e8f; */
    padding-right: 1px;
    margin-left: -6px;
    max-height: 217px;">
                                        <div class="block_content">
                                            
                                            <div style="padding-top:9px">
                                                <div class="contato-form" style="box-shadow: 0px 0px 14px #cacaca;float: none;"> 
                                                    <h2 class="" ><a style="float: left;font-size: 28px;color: #1b2872;margin-left: 0px;text-shadow: 0px 0px 4px #adb9c5;text-transform: capitalize;"><label style="font-family:Gilroy ExtraBold;color: #1abb9c;">Titulo: </label><%=atividade.titulo%>.</a>
                                                    
                                                    <div class="<%=atividade.nivel%>" style="text-align: center;width:12%;float:right;background: #1e262e;border-radius: 18px;">   
                                                        <span style="padding: 2px;padding-left: 15px;padding-right: 15px;font-family:Gilroy ExtraBold">Nivel - <%=atividade.nivel%>. </span>
                                                    </div>
                                                    <div class="clearfix"></div>
                                                    </h2>
                                                    <div class="byline">
                                                        <span><%=atividade.agendamento.strftime("%d/%m/%Y as %H:%M ")%></span><span>, Tipo: <%=atividade.tipo%>. </span>
                                                        <br>  
                                                    </div> 
                                                    <label style="font-family:Gilroy ExtraBold;color: #1abb9c;">Tipo:</label>
                                                    <p style="display:inline">
                                                        <%=atividade.tipo%>.
                                                    </p>
                                                    <br>
                                                    <p style="display:block">
                                                        <label style="font-family:Gilroy ExtraBold;color: #1abb9c;">Resumo:</label>
                                                        <%=atividade.resumo.unpack(("a%i" % 50) * (atividade.resumo.size / 50.0).ceil).join("\n").force_encoding("UTF-8")%>.
                                                    </p>
                                                    <%= link_to 'Visualizar', atividade, class:"modal__btn modal__btn-primary", style:"margin-top: -27px;font-family: Gilroy ExtraBold;font-size: 16px;float: right; border-radius: 8px;"%></a>
                                                </div> 
                                            </div>
                                            <br>
                                            </p>
                                            <a>
                                        </div>
                                    </div>   
                                </li>  
                            <% end %> 
                        <% end %>
                    <% end %>
                </ul>
