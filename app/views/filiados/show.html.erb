<div class="x_panel" style="font-size:16px">
  <div class="x_title">
    <h2 style="width:100%">Atendimento
      <small style="float:right">
      <%= link_to 'Editar', edit_filiado_path(@filiado) ,class:"fa fa-pencil-square-o"%>
      <%= link_to 'Voltar', filiados_path,class:"fa fa-reply" %>
      </small>
    </h2>
      
    <div class="clearfix"></div>
  </div>
  <div class="x_content">
  <div class="" style="text-align:center;width: 100%;">
