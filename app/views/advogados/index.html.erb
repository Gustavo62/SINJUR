<p id="notice"><%= notice %></p>

<h1>Advogados</h1>

<table>
  <thead>
    <tr>
      <th>Nome</th>
      <th>Endereço</th>
      <th>Cidade</th>
      <th>Bairro</th>
      <th>Cep</th>
      <th>Telefone</th>
      <th>Cidade</th>
      <th>Email</th>
      <th>Advogado</th>
      <th>Oab</th>
      <th>Telefone</th>
      <th>Observação</th>
      <th>Cadastro atendimento</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @advogados.each do |advogado| %>
      <tr>
        <td><%= advogado.nome %></td>
        <td><%= advogado.endereço %></td>
        <td><%= advogado.cidade %></td>
        <td><%= advogado.bairro %></td>
        <td><%= advogado.cep %></td>
        <td><%= advogado.telefone %></td>
        <td><%= advogado.cidade %></td>
        <td><%= advogado.email %></td>
        <td><%= advogado.advogado %></td>
        <td><%= advogado.oab %></td>
        <td><%= advogado.telefone %></td>
        <td><%= advogado.observação %></td>
        <td><%= advogado.cadastro_atendimento %></td>
        <td><%= link_to 'Show', advogado %></td>
        <td><%= link_to 'Edit', edit_advogado_path(advogado) %></td>
        <td><%= link_to 'Destroy', advogado, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Advogado', new_advogado_path %>
