
<div class="nested-fields"style="
    padding: 11px; 
    background-color: aliceblue;
    border: 1px solid; 
    margin-top: 12px;
    border-radius: 2px;
    text-align: left;
">
    <div class="field">
    <div style="text-align:right;"><%= link_to_remove_association('', f,class:"fa fa-remove")%></div>
        <div class="field">
            <%= f.label :area_atuacao %>:
            <%= f.text_field :area_atuacao,style:"width:30%" %> 
            <%= f.label :objeto_acao %>:
            <%= f.text_field :objeto_acao,style:"width:25%" %> 
            <%= f.label :assunto %>:
            <%= f.text_field :assunto,style:"width:20%" %> 
            <%= f.label :detalhe %>:
            <%= f.text_field :detalhe, style:"width:93%;margin-left: -2px;"%>
        </div>
        <br>
         <div class="field"> 
            <%= f.label :local_tramite %> 1:
            <%= f.text_field :local_tramite_um,style:"width:39%;"%> 
            <%= f.label :local_tramite %> 2:
            <%= f.text_field :local_tramite_dois,style:"width:40%;margin-left: -4px;" %>
            
        </div>
        <br> 
        <div class="field">
            <%= f.label :pasta %>:
            <%= f.text_field :pasta,style:"width:94%;" %> 
            <%= f.label :etiqueta %>:
            <%= f.text_field :etiqueta,style:"width:92.5%" %>
        </div> 
        <br> 
        <div class="field"> 
            <%= f.label :status_processo %>:
            <%= f.select :status_processo, ['Aberto', 'Em processamento', 'Concluido'] %> 
            Favorito: <%= f.check_box :favorito %>
        </div>
    </div>
</div>